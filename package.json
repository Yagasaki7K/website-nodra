{
	"name": "website-nodra",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"vercel": "next build",
		"start": "next start",
		"lint": "next lint",
		"create:mylint": "bunx --bun biome init && mylint",
		"mylint": "bunx biome format --write",
		"commit": "git pull && bun run vercel && bun mylint && read -p 'Descriptions Commit: ' message && git add . && echo -n \"$message - \" && node getTime.js | xargs -I {} git commit -m \"$message - {}\" && git push"
	},
	"dependencies": {
		"next": "15.3.4",
		"next-seo": "^6.8.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"styled-components": "^6.1.19"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.1",
		"@types/node": "^20.19.1",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"typescript": "^5.8.3"
	}
}
